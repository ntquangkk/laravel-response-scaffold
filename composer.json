{
  "name": "triquang/laravel-response-scaffold",
  "description": "Generate response scaffolding and exception handler.",
  "version": "2.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyễn Trí Quang",
      "email": "ntquangkk@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "TriQuang\\LaravelResponseScaffold\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TriQuang\\LaravelResponseScaffold\\LaravelResponseScaffoldServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "standardize",
    "api",
    "response",
    "exception",
    "handler",
    "redirect",
    "scaffold",
    "generator"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
